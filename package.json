    "bootstrap": "^5.3.3",
    "canvas-confetti": "^1.9.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-image-lightbox": "^5.1.4",
    "react-tsparticles": "^2.12.2",
    "react-use": "^17.5.0",
    "sass": "^1.72.0",
    "tsparticles": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
